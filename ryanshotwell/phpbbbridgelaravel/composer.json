{
    "name": "ryanshotwell/phpbbbridgelaravel",
    "type": "phpbb-extension",
    "description": "An authentication bridge connecting phpBB and Laravel",
    "homepage": "https://github.com/ryanshotwell/phpBB-Auth-Bridge-Laravel",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Shotwell",
            "email": "contact@ryanshotwell.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
      "display-name": "Laravel Authentication Bridge",
      "soft-require": {
        "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      }
    }
}
